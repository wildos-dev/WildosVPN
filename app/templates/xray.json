{
    "log": {
        "loglevel": "warning"
    },
    "dns": {
        "queryStrategy": "UseIP",
        "servers": [
            "1.1.1.1",
            "8.8.8.8"
        ],
        "tag": "dns_out"
    },
    "routing": {
        "domainStrategy": "IPIfNonMatch",
        "rules": [
            {
                "domain": [
                    "geosite:category-ads-all"
                ],
                "outboundTag": "block"
            },
            {
                "domain": [
                    "regexp:.*\\.ir",
                    "domain:ir",
                    "geosite:private"
                ],
                "outboundTag": "direct"
            },
            {
                "ip": [
                    "geoip:ir",
                    "geoip:private"
                ],
                "outboundTag": "direct"
            }
        ]
    },
    "policy": {
        "system": {
            "statsOutboundDownlink": true,
            "statsOutboundUplink": true
        }
    },
    "inbounds": [
        {
            "port": 10808,
            "protocol": "socks",
            "settings": {
                "auth": "noauth",
                "udp": true,
                "userLevel": 8
            },
            "sniffing": {
                "destOverride": [
                    "http",
                    "tls",
                    "fakedns"
                ],
                "enabled": true
            },
            "tag": "socks"
        },
        {
            "port": 10809,
            "protocol": "http",
            "settings": {
                "userLevel": 8
            },
            "tag": "http"
        }
    ],
    "outbounds": [
        {
            "tag": "direct",
            "protocol": "freedom"
        },
        {
            "tag": "block",
            "protocol": "blackhole"
        }
    ],
    "stats": {}
}
